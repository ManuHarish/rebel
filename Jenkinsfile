pipeline
{
  agent any
  stages
  {
      stage ('clonoing code from github')
      {
          steps
          {
            sh 'touch bbb'
          }
      }
       stage ('building the code')
      {
          steps
          {
            sh 'manipulate abc'
          }
      }
      stage ('testing the code')
      {
          steps
          {
            sh 'touch bac'
          }
      }
      stage ('deploying the code')
      {
          steps
          {
           sh 'touch cab'
          }
      }
     
  }
}
